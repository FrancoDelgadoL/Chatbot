define(['jquery','uiComponent','Magento_Customer/js/customer-data',],function($,Component,customerData){'use strict';return Component.extend({initialize:function(){this._super();let userData=customerData.get('customer');userData.subscribe(function(data){if(data.loyalty_points){$('.loyalty-points').text(data.loyalty_points+' Pts').parents('.points-btn').addClass('active-points');}}.bind(this));}});});